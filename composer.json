{
    "name": "bentools/easy-form-bundle",
    "description": "Easily generate small Symfony forms (delete / toggle buttons)",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "doctrine/common": "^2.5",
        "symfony/symfony": "^2.8|^3.0|^4.0",
        "sylius/resource": "^1.0",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BenTools\\EasyFormBundle\\": "src"
        }
    },
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
